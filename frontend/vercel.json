{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],  "env": {
    "VITE_API_URL": "https://guruinversor-v1-production.up.railway.app"
  },
  "build": {
    "env": {
      "VITE_API_URL": "https://guruzinversor-backend.railway.app"
    }
  }
}
